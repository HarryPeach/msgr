{
	"hosting": {
		"public": "out",
		"ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
		"rewrites": [
			{
				"source": "**",
				"destination": "/index.html"
			}
		]
	},
	"firestore": {
		"rules": "/src/firebase/firestore.rules",
		"indexes": "/src/firebase/firestore.indexes.json"
	}
}
